" v-bind:to=\"{ nombre: 'view-contact', params: { person: prof.slug }}\">Ver profesor</router-link>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n\n</template>\n\n<script>\n  import { db } from '../config/db';\n\n  export default {\n    name: 'home',\n    data () {\n      return {\n        profes: [],\n        loading: true\n      }\n    },\n    created () {\n      db.collection('profes').get().then((querySnapshot) => {\n        this.loading = false\n        querySnapshot.forEach((doc) => {\n          let data = {\n            'nombre': doc.data().nombre,\n            'apellido': doc.data().apellido,\n            'email': doc.data().email,\n            'whatsapp': doc.data().whatsapp,\n            'uuid': doc.data().uuid\n          }\n          this.contacts.push(data)\n        })\n      })\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  h1 {\n    font-size: 30px;\n    margin: 30px 0;\n  }\n  .user-list {\n    margin-top: 30px;\n    background-color: white;\n    padding: 20px;\n    box-shadow: 0 0 5px 0 rgba(0,0,0,0.05);\n    .column {\n      height: 95px;\n    }\n    .inner {\n      .left {\n        width: 50%;\n        float: left;\n        text-align: left;\n      }\n      .right {\n        width: 50%;\n        float: left;\n        text-align: left;\n        p {\n          width: 100%;\n          text-align: left;\n        }\n      }\n    }\n    .right {\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      button {\n        background: #4B75FF;\n      }\n    }\n    .user-list__header {\n      font-size: 20px;\n      font-weight: 700;\n    }\n    .user-list__sub {\n      font-size: 15px;\n      margin-top: 10px;\n    }\n  }\n  @keyframes placeHolderShimmer{\n    0%{\n      background-position: -468px 0\n    }\n    100%{\n      background-position: 468px 0\n    }\n  }\n  .animated-background__header {\n    -webkit-animation-duration: 1s;\n    animation-duration: 1s;\n   